{
    "id": "mail",
    "name": "Mail.ru",
    
    "type": "html",
    
    "dist": {
        "format": "zip",
        "maxSize": 150
    },
    
    "requiredFiles": ["index.html"],
    "allowedFiles": ["*.html", "*.png", "*.jpg", "*.jpeg", "*.gif", "*.css", "*.js", "*.svg"],
    "maxFilesNum": 0,
    
    "indexFile": "index.html",
    
    "externalLinks": false,
    
    "fallback": {
        "required": false
    },
    
    "tags": {
        "head": [
            {
                "tag": "meta",
                "attributes": {
                    "name": "ad.size",
                    "content": "width={{banner.width}},height={{banner.height}}"
                }
            }
        ],
        "body": []
    },
    
    "attributes": {
        "#link": {
            "target": "_blank",
            "href": "%link1%"
        }
    },
    
    "inlineFiles": false,
    "minify": true,
    
    "browsers": {},
    
    "browsers": {
        "chrome": 18,
        "firefox": 14,
        "safari": 5,
        "ie": 10
    }
}
