{
    "id": "adfox",
    "name": "Adfox",
    
    "type": "html",
    
    "dist": {
        "format": "zip",
        "maxSize": 15000
    },
    
    "requiredFiles": ["index.html"],
    "allowedFiles": ["*.*"],
    "maxFilesNum": 50,
    
    "indexFile": "index.html",
    
    "externalLinks": true,
    
    "fallback": {
        "required": false
    },
    
    "tags": {
        "head": [],
        "body": []
    },
    
    "attributes": {
        "body": {
            "data-version": "v5"
        },
        "#link": {
            "href": "%banner.reference_user1%",
            "target": "%banner.target%",
            "aria-label": "Перейти по ссылке в баннере"
        },
        "#container": {
            "aria-hidden": "true"
        }
    },
    
    "inlineFiles": false,
    "minify": true,
    
    "browsers": {}
}
