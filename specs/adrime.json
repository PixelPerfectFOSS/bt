{
    "id": "adrime",
    "name": "AdRime",
    
    "type": "html",
    
    "dist": {
        "format": "zip",
        "maxSize": 150
    },
    
    "requiredFiles": ["index.html"],
    "allowedFiles": ["*.html", "*.js", "*.css", "*.jpg", "*.jpeg", "*.gif", "*.png", "*.svg"],
    "maxFilesNum": 0,
    
    "indexFile": "index.html",
    
    "externalLinks": false,
    
    "fallback": {
        "required": false
    },
    
    "tags": {
        "head": [
            {
                "tag": "script",
                "attributes": {
                    "src": "//media.adrcdn.com/scripts/screenad_interface_1.0.3_scrambled.js"
                },
                "text": ""
            },
            {
                "tag": "comment",
                "attributes": {},
                "text": " ADRIME SCREENAD META DATA (don't edit/remove) "
            },
            {
                "tag": "comment",
                "attributes": {},
                "text": " SCRVERSION: screenad_interface_1.0.3 "
            },
            {
                "tag": "comment",
                "attributes": {},
                "text": " SCRFORMAT: banner "
            },
            {
                "tag": "comment",
                "attributes": {},
                "text": " SCRWIDTH: {{banner.width}} "
            },
            {
                "tag": "comment",
                "attributes": {},
                "text": " SCRHEIGHT: {{banner.height}} "
            }
        ],
        "body": [
        ]
    },
    
    "attributes": {
        "#link": {
            "href": null,
            "target": null,
            "onclick": "screenad.click('default');",
            "style": "cursor: pointer;"
        },
        "#container": {
            "aria-hidden": "true"
        }
    },
    
    "ids": {
    },
    
    "inlineFiles": false,
    "minify": true,
    
    "browsers": {
    }
}
