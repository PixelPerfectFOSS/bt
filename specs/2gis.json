{
    "id": "2gis",
    "name": "2GIS",
    
    "type": "html",
    
    "dist": {
        "format": "zip",
        "maxSize": 950
    },
    
    "requiredFiles": ["index.html"],
    "allowedFiles": ["*.html"],
    "maxFilesNum": 1,
    
    "indexFile": "index.html",
    
    "externalLinks": false,
    
    "animation": {
        "duration": 40
    },
    
    "fallback": {
        "required": false
    },
    
    "tags": {
        "head": [],
        "body": []
    },
    
    "attributes": {
        "#link": {
            "target": "_blank"
        },
        "#container": {
            "aria-hidden": "true"
        }
    },
    
    "ids": {},
    
    "inlineFiles": true,
    "minify": true
}
