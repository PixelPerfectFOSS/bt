{
    "id": "wifi",
    "name": "WiFi",
    
    "type": "html",
    
    "dist": {
        "format": "zip",
        "maxSize": 3072
    },
    
    "requiredFiles": ["index.html"],
    "allowedFiles": ["*.html", "*.js", "*.css", "*.jpg", "*.gif", "*.png", "*.svg"],
    
    "maxFilesNum": 10,
    
    "indexFile": "index.html",
    
    "externalLinks": false,
    
    "fallback": {
        "required": false
    },
    
    "tags": {
        "head": [],
        "body": []
    },
    
    "attributes": {
        "#link": {
            "href": null,
            "target": null,
            "onclick": "parent.postMessage('banner.redirect', '*');"
        },
        "#container": {
            "aria-hidden": "true"
        }
    },
    
    "inlineFiles": false,
    "minify": true,
    
    "browsers": {}
}
