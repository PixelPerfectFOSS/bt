{
    "id": "studio",
    "name": "DoubleClick Studio",
    
    "type": "html",
    
    "dist": {
        "format": "zip",
        "maxSize": 150
    },
    
    "requiredFiles": ["index.html"],
    "allowedFiles": ["*.html", "*.htm", "*.js", "*.css", "*.jpg", "*.jpeg", "*.gif", "*.png", "*.json", "*.xml", "*.svg", "*.eot", "*.otf", "*.ttf", "*.woff", "*.woff2", "*.webp", "*.tte"],
    
    "maxFilesNum": 40,
    
    "indexFile": "index.html",
    
    "externalLinks": true,
    "externalLinksDomain": "s0.2mdn.net",
    
    "animation": {
        "duration": 30
    },
    
    "fallback": {
        "required": true,
        "maxSize": 150,
        "formats": ["gif", "png", "jpg"]
    },
    
    "tags": {
        "head": [
            {
                "tag": "meta",
                "attributes": {
                    "name": "ad.size",
                    "content": "width={{banner.width}},height={{banner.height}}"
                }
            },
            {
                "tag": "script",
                "attributes": {
                    "src": "https://s0.2mdn.net/ads/studio/Enabler.js"
                },
                "text": ""
            }
        ],
        "body": [
            {
                "tag": "script",
                "attributes": {},
                "text": "\r\n{{#load}}studio.js{{/load}}"
            }
        ]
    },
    
    "attributes": {
        "#link": {
            "target": null,
            "href": null,
            "style": "cursor: pointer;"
        },
        "#container": {
            "aria-hidden": "true"
        }
    },
    
    "ids": {},
    
    "inlineFiles": false,
    "minify": true,
    
    "browsers": {
        "chrome": 14,
        "firefox": 14,
        "safari": 5,
        "ie": 9,
        "opera": 14,
        "yandex": 14,
        "edge": 0
    }
}
